        "import pandas as pd\n",
        "\n",
        "# สมมุติว่าคุณมี DataFrame ชื่อ df ที่มีข้อมูลทั้งหมด\n",
        "df['PolicyDate'] = pd.to_datetime(df['PolicyDate'])\n",
        "\n",
        "# กรองข้อมูลที่ TYPE เป็น \"ต่ออายุ\"\n",
        "df_renewal = df[df['TYPE'] == 'ต่ออายุ']\n",
        "\n",
        "# สร้างคอลัมน์ 'Year' และ 'Day of Year' เพื่อแยกข้อมูลในปีที่แล้ว\n",
        "df_renewal['Year'] = df_renewal['PolicyDate'].dt.year\n",
        "df_renewal['DayOfYear'] = df_renewal['PolicyDate'].dt.dayofyear\n",
        "\n",
        "# สร้าง DataFrame สำหรับการต่ออายุในวันเดียวกันของปีก่อน\n",
        "df_previous_year = df_renewal[df_renewal['Year'] == (df_renewal['Year'].max() - 1)]\n",
        "df_previous_year = df_previous_year.groupby('DayOfYear').size()\n",
        "\n",
        "# คำนวณ Moving Average จากสัปดาห์ที่ผ่านมา (ใช้ข้อมูลสัปดาห์ก่อนหน้า)\n",
        "df_renewal.set_index('PolicyDate', inplace=True)\n",
        "df_renewal_weekly = df_renewal.resample('W').size()  # จำนวนการต่ออายุในแต่ละสัปดาห์\n"
      ]
    }
  ]
